			   ( ExecutionId	
			    ,ExecutableId	
			    ,EnvironmentId	
				,EntityId	
				,LoadProcess	
				,TaskName	
				,ProcessStream	
				,LoadId	
				,ApplicationId	
				,SourceFileName	
				,InsertedRows	
				,ExtractedRows
				,UpdatedRows	
				,DeletedRows	
				,FailedRows	
				,ArchivedRows	
				,BadRows	
			   )
		 VALUES
			   ( " + (DT_WSTR, 10) @[System::ServerExecutionID]  + "-- ExecutionId	
				,-1 -- EntityId	
			   	,-1
				,-1
				,'"  +  @[User::strProcessStream]  + "' --LoadProcess	
				,'" + @[System::TaskName]  + "'	--TaskName
				, '" + @[System::PackageName] + " Load" + "'
				," + (DT_WSTR, 10)@[User::intParentLoadId] + "	
				,@ApplicationId	
				,'" + @[User::strWorkingStagingTable]   + "' 
				," + (DT_WSTR, 10) @[User::intInsertedRows] + "
				,0,0,0,0,0,0
			   )
"</DTS:PropertyExpression>
