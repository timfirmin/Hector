          DTS:ThreadHint="2">
          <DTS:Variables />
          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">"UPDATE fl
SET
 TransformTotalRows = sm.StagedRowCount
,TransformValidValue = sm.StagedCheckValue
,TransformTimeStamp = GetDate()
FROM [DDIstaging].[FileLog] fl 
JOIN 
(SELECT 
	  [FileLogKey] as StagedFileLogKey
      ,sum(" + @[User::strWorkingRowCheckField] + ") as StagedCheckValue
      ,count(*) as StagedRowCount
  FROM FinanceDataMart." +  @[User::strWorkingFDMTable]  + "
  where [InsertLoadId] = " + (DT_WSTR, 7)@[User::intParentLoadId]    + "
  and [DistributionGroupCode] = '" +  @[User::strWorkingDistributionGroupCode]  + "'
  and [DistributionCode] = '" +  @[User::strWorkingDistributionCode]  + "'
  and [CompanyCode] = '" +  @[User::strWorkingCompanyCode]  + "'
group by [FileLogKey]) sm ON fl.[FileLogUniqueKey] = sm.StagedFileLogKey"</DTS:PropertyExpression>
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{69A6A162-6A37-42BB-BD9C-BEF90A7B1ACB}"
              SQLTask:SqlStatementSource="UPDATE fl&#xA;SET&#xA; TransformTotalRows = sm.StagedRowCount&#xA;,TransformValidValue = sm.StagedCheckValue&#xA;,TransformTimeStamp = GetDate()&#xA;FROM [DDIstaging].[FileLog] fl &#xA;JOIN &#xA;(SELECT &#xA;&#x9;  [FileLogKey] as StagedFileLogKey&#xA;      ,sum(TotalPRAllocation) as StagedCheckValue&#xA;      ,count(*) as StagedRowCount&#xA;  FROM FinanceDataMart.DDI.DistributionPublicReception&#xA;  where [InsertLoadId] = 76112&#xA;  and [DistributionGroupCode] = 'ILR'&#xA;  and [DistributionCode] = 'D171'&#xA;  and [CompanyCode] = 'PRS'&#xA;group by [FileLogKey]) sm ON fl.[FileLogUniqueKey] = sm.StagedFileLogKey" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
