        <DTS:Executable
          DTS:refId="Package\SEQC200 - Staging Load\SQL230 - Perform Basic Validation"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{90b9f7fd-e32a-4b56-aaab-014a61fba67e}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:ExecValue="1"
          DTS:ExecValueType="3"
          DTS:ForceExecValue="True"
          DTS:LocaleID="-1"
          DTS:MaxErrorCount="0"
          DTS:ObjectName="SQL230 - Perform Basic Validation"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="1">
          <DTS:Variables />
          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">"with cte_CheckDupes as
(
SELECT 
	  [FileLogKey] as StagedFileLogKey
      ,count(distinct StationCode) as UniqueStationCodes
  FROM [DDIstaging].[DistributionPublicReception]
  where [InsertLoadId] = " + (DT_WSTR, 7)@[User::intParentLoadId]    + "
  and [DistributionGroupCode] = '" +  @[User::strWorkingDistributionGroupCode]  + "'
  and [DistributionCode] = '" +  @[User::strWorkingDistributionCode]  + "'
  and [CompanyCode] = '" +  @[User::strWorkingCompanyCode]  + "'
group by [FileLogKey]
)
UPDATE fl
SET FileValidationComment = left(FileValidationComment + CASE WHEN fl.[FileRawRowCount] &gt; cte.UniqueStationCodes THEN 

'| ' + cast(fl.[FileRawRowCount]- cte.UniqueStationCodes as varchar(5))  + ' Duplicate / Empty Station Code(s) Found ' ELSE '' END,255)

,FileValidFlag = 'N'
FROM [DDIstaging].[FileLog] fl JOIN cte_CheckDupes cte ON fl.[FileLogUniqueKey] = cte.StagedFileLogKey
WHERE  fl.[FileRawRowCount] &gt; cte.UniqueStationCodes;



with cte_CheckValues as
(
SELECT 
 		[FileLogKey] as StagedFileLogKey
      ,SUM(CASE WHEN ISNUMERIC(ltrim(rtrim(replace(replace(replace(TotalPRAllocation,' ',''),'-','0'),'£','')))) = 1 THEN 1 ELSE 0 END) as ValidMeasureCount
  FROM [DDIstaging].[DistributionPublicReception] dpr
  where [InsertLoadId] = " + (DT_WSTR, 7)@[User::intParentLoadId]    + "
	  and [DistributionGroupCode] = '" +  @[User::strWorkingDistributionGroupCode]  + "'
	  and [DistributionCode] = '" +  @[User::strWorkingDistributionCode]  + "'
	  and [CompanyCode] = '" +  @[User::strWorkingCompanyCode]  + "'
  GROUP BY [FileLogKey]
)
UPDATE fl
SET FileValidationComment = left(FileValidationComment + CASE WHEN fl.[FileRawRowCount] &gt; cte.ValidMeasureCount THEN '| ' + cast(fl.[FileRawRowCount]- cte.ValidMeasureCount as varchar(5)) + ' Invalid Measures Found ' ELSE '' END,255)
,FileValidFlag = 'N'
FROM [DDIstaging].[FileLog] fl JOIN cte_CheckValues cte ON fl.[FileLogUniqueKey] = cte.StagedFileLogKey
WHERE  fl.[FileRawRowCount] &gt; cte.ValidMeasureCount and charindex('Duplicate',FileValidationComment,1) = 0;"</DTS:PropertyExpression>
