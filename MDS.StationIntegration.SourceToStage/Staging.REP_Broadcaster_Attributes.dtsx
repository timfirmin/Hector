                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
 dbap.purpose_code
,db.domain
,db.station_id
,db.broadcaster

,dba.licence_group
,dba.music_reporting_group
,dba.key_account_group

FROM prbi_dw.dim_broadcaster db
JOIN     prbi_dw.fact_bro_attributes fba       ON (fba.broadcaster_key          = db.broadcaster_key)
JOIN prbi_dw.dim_bro_attribute_purpose dbap      ON (dbap.bro_attribute_purpose_key           = fba.bro_attribute_purpose_key)
JOIN prbi_dw.dim_broadcaster_attributes dba ON (dba.broadcaster_attributes_key = fba.broadcaster_attributes_key)

WHERE db.domain IN ('Television', 'Radio', 'GEOD') and dbap.purpose_code &gt; 'D170' and dbap.purpose_code like 'D%'
order by 1,2,3</property>
