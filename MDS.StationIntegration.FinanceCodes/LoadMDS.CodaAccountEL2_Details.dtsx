DECLARE
 @UserName nvarchar(50) = '" + @[User::strMdsUserName]  + "'
,@ModelName nvarchar(50) = N'" + @[User::strMdsModelName]  + "'
,@UserID INT
,@ModelID INT
,@VersionID INT

